      <image class="leftImage" src="{{imgPath}}2.png" background-size="cover"></image>
      <text>我的商铺</text>
    </view>
    <image class="funImage" src="{{rightImg}}" background-size="cover"></image>
  </view>
  <view class="funListItem" bindtap="myPub">
    <view class="left">
      <image class="leftImage" src="{{imgPath}}2.png" background-size="cover"></image>
      <text>我的说说</text>
    </view>
    <image class="funImage" src="{{rightImg}}" background-size="cover"></image>
  </view>
  <view class="spaceView">
  </view>
  <view class="funListItem" bindtap="pubj">
