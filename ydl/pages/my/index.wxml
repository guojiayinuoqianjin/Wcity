<view class="container">
  <view class="headInfo">
    <image class="myImage" src="{{userInfo.avatarUrl}}" background-size="cover"    bindtap="pre" data-imgs="{{[userInfo.avatarUrl]}}" ></image>
    <text>{{userInfo.nickName}}</text>
  </view>
  <view class="funListItem" bindtap="myPub">
    <view class="left">
      <image class="leftImage" src="{{imgPath}}2.png" background-size="cover"></image>
      <text>我的发布</text>
    </view>
    <image class="funImage" src="{{rightImg}}" background-size="cover"></image>
  </view>
  <view class="funListItem" bindtap="myPub">
