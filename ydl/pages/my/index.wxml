      <image class="leftImage" src="{{imgPath}}3.png" background-size="cover"></image>
      <text>联系客服</text>
    </view>
    <contact-button type="default-dark" size="27" session-from="weapp"   >  
    </contact-button>
  </view>
  <view class="weui-footer weui-footer_fixed-bottom">
    <view class="weui-footer__links">
      <navigator url="" class="weui-footer__text">{{wappName}}</navigator>
